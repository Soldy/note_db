CREATE OR REPLACE FUNCTION `noteUpdate`(
  IID INT UNSIGNED,
  IHIGH VARCHAR(16),
  ITITLE VARCHAR(128),
  ITEXT TEXT
) RETURNS INT MODIFIES SQL DATA
BEGIN
    DECLARE title_id INT UNSIGNED; 
    DECLARE text_id INT UNSIGNED; 
    DECLARE high_id INT UNSIGNED; 
    SELECT `note_highlightUpdate`(IID, ITITLE) INTO high_id;
    SELECT `note_titleUpdate`(IID, ITITLE) INTO title_id;
    SELECT `note_textUpdate`(IID, ITEXT) INTO text_id;
    RETURN IID;
END;
