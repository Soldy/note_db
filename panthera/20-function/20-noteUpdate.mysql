CREATE OR REPLACE FUNCTION `noteUpdate`(
  IID BIGINT UNSIGNED,
  IHIGH VARCHAR(16),
  ITITLE VARCHAR(128),
  ITEXT TEXT
) RETURNS BIGINT MODIFIES SQL DATA
BEGIN
    DECLARE title_id BIGINT UNSIGNED; 
    DECLARE text_id BIGINT UNSIGNED; 
    DECLARE high_id BIGINT UNSIGNED; 
    SELECT `note_highlightUpdate`(IID, ITITLE) INTO high_id;
    SELECT `note_titleUpdate`(IID, ITITLE) INTO title_id;
    SELECT `note_textUpdate`(IID, ITEXT) INTO text_id;
    RETURN IID;
END;
