
/**
 * @ptable note_urls
 * @pcol {serial} id 
 * @pcol {uint64_t} note
 * @pcol {string} url
 * @pcol {string} title
**/

CREATE TABLE `note_urls` (
    `id` SERIAL,
    `note` BIGINT UNSIGNED NOT NULL,
    `url`  VARCHAR(128),
    `title`  VARCHAR(128),
     KEY `index` (`id`)
)  ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;;
