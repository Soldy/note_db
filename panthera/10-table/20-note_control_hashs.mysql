
/**
 * @ptable note_control_hashs
 * @pcol {serial} id 
 * @pcol {uint64_t} note
 * @pcol {string} hash
**/

CREATE TABLE `note_control_hashs` (
    `id` SERIAL,
    `note` BIGINT UNSIGNED NOT NULL,
    `hash` VARCHAR(128),
     KEY `index` (`id`)
)  ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;;
